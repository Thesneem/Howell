<!-- categories carousal ends -->
<div class="text-center">
    <h3 class="mb-5">Categories</h3>
</div>

<div class="container">
    <div class="row">
      <% category.forEach(function(cat){ %>
        <div class="col-md-3">
            <a href="/shoppage">
          <div class="card mb-3 align-items-center">
            <img src="/admin/images/<%=cat.image%>"  class="card-img-top" alt="..." style="height:150px;width:150px">
            <div class="card-body text-center">
              <h5 class="card-title fw-bold"> <%= cat.categoryName %></h5>
              <p class="card-text"><%=cat.Description%></p>
              <a href="/shoppage" class="btn btn-danger mt-2">Browse More</a>
            </div>
          </div>
        </div>
    </a>
      <% }) %>
    </div>
  </div>
   

  <section class="text-center">
    <h3 class="mb-3">Top Brands</h3>
    <div class="container-fluid">
    <div class="row text-center py-4 mb-4" style="background-color: brown;">
            
        
        <!-- <div class="col-md-12  text-center brands">

          <div class="container">
            <div class="row justify-content-center"> -->
                
                <% brands.forEach(function(brand){ %>
                    
              <div class="col-sm-6 col-md-3 text-center text-white fw-bold">
                <h4><%=brand.brand%></h4>
              </div>
              
              <% }) %>
        

            </div>
          </div>
          
          
            </div>
        </div>
    </div>
</section>
            
            <!-- Categories Section End -->

            <!-- Product Section Begin -->
            <section class="product spad">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-4 col-md-4">
                            <div class="section-title">
                                <h4>New products</h4>
                            </div>
                        </div>

                    </div>
                    <div class="row property__gallery">
                        <% product.forEach(function(product){ %>
                            <div class=" col-md-3 col-sm-6 mix women">

                                <!-- <a href="/shoppage"> -->
                                    <div class="product__item">
                                        <div class="product__item__pic set-bg"
                                            data-setbg="/admin/images/<%=product.image[0]%>" style="height:200px;width:200px">
                                            <!-- <div class="label new">New</div> -->
                                            <ul class="product__hover">
                                                <li><a href="/admin/images/<%=product.image[0]%>"
                                                        class="image-popup"><span class="arrow_expand"></span></a></li>
                                                        <li><a class="btn btn-sm btn-outline-dark" href="#!"><span class="icon_heart_alt" onclick="wishlist('<%= product._id %>')"></span></a></li>

                                                <!-- <li><a href="#"><span class="icon_bag_alt"></span></a></li> -->
                                            </ul>
                                        </div>
                                        <div class="product__item__text">
                                            <h6><a href="/shoppage">
                                                    <%= product.ProductName %>
                                                </a></h6>

                                            <div class="product__price">$<%= product.Price %>
                                            </div>
                                        </div>

                                    </div>
                                <!-- </a> -->
                            </div>
                            <% }) %>

                    </div>

                </div>
                </div>
          
            </section>
            <!-- Product Section End -->

           
       
